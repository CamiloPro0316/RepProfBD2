                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT MAX(winners.bettime) maxBetTime, CONVERT(VARCHAR(10), 
DATENAME(month, winners.bettime)) mesname, 
CONVERT(INT, DATEPART(year, winners.bettime)) year,  
CONVERT(DECIMAL(18, 2), SUM(winners.amount)) MontoApostado, CONVERT(VARCHAR(30), teams.teamname) teamname,
CONVERT(SMALLINT, DATEPART(month, winners.bettime)) mesnumber
FROM 
(
	SELECT bettime, amount, 
	CASE 
		WHEN bets.visitorgoals&gt;=bets.homegoals THEN match.visitorid ELSE match.homeid END TeamId
	FROM dbo.fut_bets bets
	INNER JOIN dbo.fut_matches match ON match.matchid = bets.matchid
) winners INNER JOIN dbo.fut_teams teams ON teams.teamid = winners.TeamId
GROUP BY teams.teamname, CONVERT(SMALLINT, DATEPART(month, winners.bettime)), DATENAME(month, winners.bettime), DATEPART(yyyy, winners.bettime)</property>
